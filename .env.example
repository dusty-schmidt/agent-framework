# Agentic Framework Environment Configuration
# Copy this file to .env and fill in your values

# OpenRouter API Configuration
OPENROUTER_API_KEY=your-openrouter-api-key-here

# Optional: Override default model
# DEFAULT_MODEL=openai/gpt-oss-20b

# Optional: Override API settings
# API_TIMEOUT=30
# DEFAULT_TEMPERATURE=0.7
# DEFAULT_MAX_TOKENS=1000

# Development Settings
DEBUG=true
LOG_LEVEL=INFO

# Database/Storage Settings (for Docker deployment)
# POSTGRES_URL=postgresql://user:pass@localhost:5432/agentic_db
# REDIS_URL=redis://localhost:6379/0

# Docker-specific settings (will be set by Docker)
# CONTAINER_NAME=agentic-framework
# ENVIRONMENT=development
